{
  "expo": {
    "name": "Nova",
    "slug": "Nova",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconnova.png",
    "scheme": "novaapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.reyzer.Nova"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/iconnova.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iconnova.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ff10b47b-abe6-4866-b07b-2c757fc4612f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ff10b47b-abe6-4866-b07b-2c757fc4612f"
    },
    "owner": "meysamalizadeh"
  }
}
